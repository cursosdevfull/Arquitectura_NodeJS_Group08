{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "dev": {
            "dependsOn": [
                "^dev"
            ],
            "cache": false
        },
        "//#biome-check": {},
        "//#biome-check:fix": {
            "cache": false
        },
        "typecheck": {
            "dependsOn": [
                "^typecheck"
            ]
        }
    }
}